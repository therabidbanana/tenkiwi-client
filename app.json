{
    "expo": {
        "name": "Tenkiwi",
        "description": "No description",
        "slug": "tenkiwi",
        "scheme": "tenkiwi",
        "version": "1.1.3",
        "orientation": "portrait",
        "primaryColor": "#cccccc",
        "privacy": "unlisted",
        "icon": "./assets/icons/app.png",
        "updates": {
            "enabled": true,
            "fallbackToCacheTimeout": 15000
        },
        "splash": {
            "image": "./assets/icons/loading.png",
            "resizeMode": "contain",
            "backgroundColor": "#000000"
        },
        "ios": {
            "bundleIdentifier": "com.davidhaslem.tenkiwi",
            "buildNumber": "1.1.3",
            "supportsTablet": false,
            "associatedDomains": [
                "applinks:play.tenkiwi.com"
            ],
        },
        "android": {
            "package": "com.davidhaslem.tenkiwi",
            "versionCode": 14,
            "permissions": [],
            "intentFilters": [
                {
                    "action": "VIEW",
                    "autoVerify": true,
                    "data": [
                        {
                            "scheme": "https",
                            "host": "play.tenkiwi.com",
                            "pathPrefix": "/"
                        },
                    ],
                    "category": [
                        "BROWSABLE",
                        "DEFAULT"
                    ]
                }
            ]

        }
    }
}
